<!DOCTYPE html>
<html>
<head>
    <title>Облік</title>
</head>
<body>

    <h1>Список обладнання</h1>

    <table border="1"> <thead>
            <tr>
                <th>ID</th>
                <th>Назва</th>
                <th>Тип</th>
                <th>Серійний номер</th>
                <th>Статус</th>
                <th>Опис</th>
            </tr>
        </thead>
        <tbody>
            <% items.forEach(function(item) { %>
                <tr>
                    <td><%= item.id %></td>
                    <td><%= item.name %></td>
                    <td><%= item.type %></td>
                    <td><%= item.serial %></td>
                    <td><%= item.status %></td>
                    <td><%= item.description %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>

</body>
</html>